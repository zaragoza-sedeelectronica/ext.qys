      lang="es">

    <head>
        <meta charset="utf-8"/>
        <sede:meta
                title="Sara Fernández Escuer (C's)"
                description="Organización Municipal, Pleno Municipal, Curriculum Vitae de Sara Fernández Escuer (C´s)"
                author="Ayuntamiento de Zaragoza"
                last-modified="Thu Oct 20 00:00:00 CEST 2016"
                keywords="Organización, Municipal, Administración Pública; government; e-administración; organización administrativa, administración local, Gobierno de Zaragoza, Sara Fernández Escuer, (C´s)">
        </sede:meta>
        <style>
            .fnd-blanco {
            background-color: #ffffff;
            padding-top: 20px;
            padding-bottom: 20px;
            }

            .fnd-gris-claro {
            padding-top: 20px;
            padding-bottom: 20px;
            margin-bottom: -40px;
            }

            .titulo-org h2,
            .procesos h2{
            color: #e83437;
            font-size: 42px;
            padding-bottom: 20px;
            }

            .titulo-org h2 small,
            .titulo-org h2 span{
            color: #000000;
            }

        </style>
    </head>

    <body>
        <sede:content container="false">
            <div class="container-fluid text-left margin-auto nomargin titulo-org">
                <h2 class="text-right">
                    <small>
                        <abbr title="Ciudadanos">C's</abbr>
                    </small>
                    <span>|</span>
                    Sara Fernández Escuer
                </h2>
            </div>
            <div class="fnd-gris-claro">
                <div class="container-fluid margin-b2em margin-t2em fnd-blanco radius">
                    <article class="row col-md-12">

                        <!-- Versión móvil -->
                        <div class="card col-xs-12 col-sm-12 hidden-md hidden-lg">
                            <div class="col-md-4 col-xs-12 text-center pull-right">
                                <img class="margin-auto img-responsive margin-b1em"
                                     src="https://www.zaragoza.es/cont/paginas/organizacion/img/concejales/C_s_Sara_Fernandez.jpg"
                                     alt="Sara Fernández Escuer"/>
                                <th:block th:include="portal/organizacion/fragment-bienes" th:with="
                    bienes=${ 
                        { 
                            {year:'2015', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/bienes/bienes-SARA-MAR%C3%8DA-FERN%C3%81NDEZ-ESCUER.pdf'},
                            {year:'2016', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/bienes/2016_Bienes_PARTIDO_CIUDADANIA/SARA_MARIA_FERNANDEZ_ESCUER.pdf'} 
