    <body>
        <sede:content container="false">
            <div class="container-fluid text-left margin-auto nomargin titulo-org">
                <h2 class="text-right">
                    <small>
                        <abbr title="Partido Socialista Obrero Español">PSOE</abbr>
                    </small>
                    <span>|</span>
                    María Dolores Ranera Gómez
                </h2>
            </div>
            <div class="fnd-gris-claro">
                <div class="container-fluid margin-b2em margin-t2em fnd-blanco radius">
                    <article class="row col-md-12">

                        <!-- Versión móvil -->
                        <div class="card col-xs-12 col-sm-12 hidden-md hidden-lg">
                            <div class="col-md-4 col-xs-12 text-center pull-right">
                                <img class="margin-auto img-responsive margin-b1em"
                                     src="https://www.zaragoza.es/cont/paginas/organizacion/img/concejales/PSOE_Lola_Ranera"
                                     alt="María Dolores Ranera Gómez"/>
                                <th:block th:include="portal/organizacion/fragment-bienes" th:with="
                    bienes=${ 
                        { 
                            {year:'2015', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/bienes/bienes-MARIA-DOLORES-RANERA-GOMEZ.pdf'},
                            {year:'2016', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/bienes/2016_Bienes_GRUPO_SOCIALISTA/MARIA_DOLORES_RANERA_GOMEZ.pdf'} 
                        } 
                    },
                    actividades=${ 
                        {
                            {year:'2015', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/actividades/activ-MARIA-DOLORES-RANERA-G%C3%93MEZ.pdf'}, 
                            {year:'2016', url: 'http://www.zaragoza.es/contenidos/organizacion/declaraciones/actividades/2016_Actividades_GRUPO_SOCIALISTA/MARIA_DOLORES_RANERA_GOMEZ.pdf'} 
                        } 
                    }"/>
                            </div>

                            <div class="panel-group col-xs-12 margin-t1em" id="accordion" role="tablist"
                                 aria-multiselectable="true">

                                <div class="panel panel-default">
                                    <div class="panel-heading padding-a1em" role="tab" id="headingPersonal">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse"
                                               data-parent="#accordion" href="#collapsePersonal" aria-expanded="false"
                                               aria-controls="collapsePersonal">
                                                INFORMACIÓN PERSONAL
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapsePersonal" class="panel-collapse collapse" role="tabpanel"
                                         aria-labelledby="headingPersonal">
                                        <ul class="panel-body list-unstyled margin-l05em">
                                            <li>
                                                <strong>Nacimiento:</strong>
                                                Zaragoza, el 24 Julio de 1970.
                                            </li>
                                            <br/>
                                            <li>Casada y con dos hijos.</li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="panel panel-default">
                                    <div class="panel-heading padding-a1em" role="tab" id="headingAcademica">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse"
                                               data-parent="#accordion" href="#collapseAcademica" aria-expanded="false"
                                               aria-controls="collapseAcademica">
                                                FORMACIÓN ACADÉMICA
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseAcademica" class="panel-collapse collapse" role="tabpanel"
                                         aria-labelledby="headingAcademica">
                                        <ul class="panel-body list-unstyled margin-l05em">
                                            <li>Licenciada en Ciencias del Trabajo y diplomada en Graduado Social por la
                                                Universidad de Zaragoza.
                                            </li>
                                            <br/>
                                            <li>Posgrado de Especialización en Técnicas de Participación Ciudadana de la
                                                Escuela de Estudios Sociales de la Universidad de Zaragoza.
                                            </li>
                                            <br/>
                                            <li>Educadora Social por el Colegio de Educadores Sociales de Galicia.</li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="panel panel-default">
                                    <div class="panel-heading padding-a1em" role="tab" id="headingPolitica">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse"
                                               data-parent="#accordion" href="#collapsePolitica" aria-expanded="false"
                                               aria-controls="collapsePolitica">
                                                EXPERIENCIA POLÍTICA
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapsePolitica" class="panel-collapse collapse" role="tabpanel"
                                         aria-labelledby="headingPolitica">
                                        <ul class="panel-body list-unstyled margin-l05em">
                                            <li>Desde diciembre de 2011, es Concejala de Régimen Interior y Concejala
                                                Delegada de Zaragoza Activa, Fomento y Turismo.
                                            </li>
                                            <br/>
                                            <li>En abril de 2012, nombrada Consejera de Participación Ciudadana y
                                                Régimen Interior, y Quinto Teniente de Alcalde, y Concejala Delegada de
                                                Zaragoza Activa, Fomento y Turismo.
                                            </li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="panel panel-default">
                                    <div class="panel-heading padding-a1em" role="tab" id="headingContacto">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse"
                                               data-parent="#accordion" href="#collapseContacto" aria-expanded="false"
                                               aria-controls="collapseContacto">
                                                CONTACTO
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseContacto" class="panel-collapse collapse" role="tabpanel"
                                         aria-labelledby="headingContacto">
                                        <ul class="panel-body list-unstyled margin-l05em">
                                            <li>
                                                <span class="far fa-envelope margin-t1em"></span>
                                                <a href="mailto:lolaranera@zaragoza.es" shape="rect">
                                                    lolaranera@zaragoza.es
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <!-- Versión desktop -->
                        <div class="hidden-xs hidden-sm col-md-12 col-lg-12">

                            <div class="col-md-4 col-xs-12 text-center pull-right">
